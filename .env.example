# Environment Variables Template
# Copy this file to .env.local and fill in your actual values
# Never commit .env.local to version control

# MongoDB Atlas Connection
MONGODB_URI=you_uri
MONGODB_DB_NAME=you_user

# Gemini API Configuration
# For Gemini native API: base URL is https://generativelanguage.googleapis.com/v1beta
# Model names: gemini-pro, gemini-1.5-pro, gemini-1.5-flash, etc.
# If using Gemini via OpenAI-compatible service (e.g., OpenRouter), use their base URL and model format
LLM_API_KEY=your_api_key_here
LLM_API_BASE_URL=https://generativelanguage.googleapis.com/v1beta
LLM_MODEL=gemini-pro

# Voyage AI for Embeddings
VOYAGE_API_KEY=your_voyage_api_key_here

